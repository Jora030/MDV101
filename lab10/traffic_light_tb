v0x5592c2ecf820_0 .net "reset", 0 0, v0x5592c2ecfad0_0;  1 drivers
E_0x5592c2e7df70 .event anyedge, v0x5592c2ecf520_0;
E_0x5592c2eb6ed0 .event posedge, v0x5592c2ecf820_0, v0x5592c2ea4f90_0;
    .scope S_0x5592c2ebb6d0;
T_0 ;
    %wait E_0x5592c2eb6ed0;
    %load/vec4 v0x5592c2ecf820_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5592c2ecf520_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5592c2ecf440_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5592c2ecf440_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5592c2ecf6f0_0;
    %assign/vec4 v0x5592c2ecf520_0, 0;
    %load/vec4 v0x5592c2ecf6f0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 5, 0, 4;
    %assign/vec4 v0x5592c2ecf440_0, 0;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x5592c2ecf440_0, 0;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x5592c2ecf440_0, 0;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5592c2ecf440_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x5592c2ecf440_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5592c2ebb6d0;
T_1 ;
    %wait E_0x5592c2e7df70;
    %load/vec4 v0x5592c2ecf520_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5592c2ecf6f0_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5592c2ecf6f0_0, 0, 2;
    %jmp T_1.4;
T_1.1 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5592c2ecf6f0_0, 0, 2;
    %jmp T_1.4;
T_1.2 ;
